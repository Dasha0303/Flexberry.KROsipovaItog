<h3 class="ui header">{{t "forms.i-i-s-k-r-osipova-itog-т-ч-акта-приемки-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номерПозиции
      class=(if (v-get validationModel "номерПозиции" "isInvalid") "error")
      label=(t "forms.i-i-s-k-r-osipova-itog-т-ч-акта-приемки-e.номерПозиции-caption")
      data-test-i-i-s-k-r-osipova-itog-т-ч-акта-приемки-e-номерПозиции=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номерПозиции" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-k-r-osipova-itog-т-ч-акта-приемки-e.склад-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.склад
      displayAttributeName="имяСклада"
      autocomplete=true
      relationName="склад"
      projection="СкладL"
      title=(t "forms.i-i-s-k-r-osipova-itog-т-ч-акта-приемки-e.склад-caption")
      readonly=readonly
      componentName="складLookup"
      class=(if (v-get validationModel "склад" "isInvalid") "error")
      data-test-i-i-s-k-r-osipova-itog-т-ч-акта-приемки-e-склад=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "склад" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-k-r-osipova-itog-т-ч-акта-приемки-e.тН-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.тН
      displayAttributeName="колво"
      autocomplete=true
      relationName="тН"
      projection="ТНL"
      title=(t "forms.i-i-s-k-r-osipova-itog-т-ч-акта-приемки-e.тН-caption")
      readonly=readonly
      componentName="тНLookup"
      class=(if (v-get validationModel "тН" "isInvalid") "error")
      data-test-i-i-s-k-r-osipova-itog-т-ч-акта-приемки-e-тН=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "тН" "message")}}
  </div>
</form>
