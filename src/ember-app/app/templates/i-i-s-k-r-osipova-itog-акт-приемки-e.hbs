      label=(t "forms.i-i-s-k-r-osipova-itog-акт-приемки-e.тЧАктаПриемки.номерПозиции-caption")
      data-test-i-i-s-k-r-osipova-itog-акт-приемки-e-тЧАктаПриемки-номерПозиции=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.тЧАктаПриемки "номерПозиции" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.тЧАктаПриемки.тН.материал
      class=(if (v-get validationModel.тЧАктаПриемки.тН "материал" "isInvalid") "error")
      label=(t "forms.i-i-s-k-r-osipova-itog-акт-приемки-e.тЧАктаПриемки.тН.материал-caption")
      data-test-i-i-s-k-r-osipova-itog-акт-приемки-e-тЧАктаПриемки-тН-материал=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.тЧАктаПриемки.тН "материал" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.тЧАктаПриемки.тН.колво
      class=(if (v-get validationModel.тЧАктаПриемки.тН "колво" "isInvalid") "error")
      label=(t "forms.i-i-s-k-r-osipova-itog-акт-приемки-e.тЧАктаПриемки.тН.колво-caption")
      data-test-i-i-s-k-r-osipova-itog-акт-приемки-e-тЧАктаПриемки-тН-колво=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.тЧАктаПриемки.тН "колво" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.тЧАктаПриемки.склад.имяСклада
      class=(if (v-get validationModel.тЧАктаПриемки.склад "имяСклада" "isInvalid") "error")
      label=(t "forms.i-i-s-k-r-osipova-itog-акт-приемки-e.тЧАктаПриемки.склад.имяСклада-caption")
      data-test-i-i-s-k-r-osipova-itog-акт-приемки-e-тЧАктаПриемки-склад-имяСклада=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.тЧАктаПриемки.склад "имяСклада" "message")}}
  </div>
</form>
