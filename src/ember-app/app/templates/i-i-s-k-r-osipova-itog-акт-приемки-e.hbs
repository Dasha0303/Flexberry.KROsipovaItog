      class=(if (v-get validationModel.тН "номерТН" "isInvalid") "error")
      label=(t "forms.i-i-s-k-r-osipova-itog-акт-приемки-e.тН.номерТН-caption")
      data-test-i-i-s-k-r-osipova-itog-акт-приемки-e-тН-номерТН=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.тН "номерТН" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.тН.поставщик
      class=(if (v-get validationModel.тН "поставщик" "isInvalid") "error")
