<h3 class="ui header">{{t "forms.i-i-s-k-r-osipova-itog-акт-приемки-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номерАкта
      class=(if (v-get validationModel "номерАкта" "isInvalid") "error")
      label=(t "forms.i-i-s-k-r-osipova-itog-акт-приемки-e.номерАкта-caption")
      data-test-i-i-s-k-r-osipova-itog-акт-приемки-e-номерАкта=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номерАкта" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.датаПриемки
      class=(if (v-get validationModel "датаПриемки" "isInvalid") "error")
      label=(t "forms.i-i-s-k-r-osipova-itog-акт-приемки-e.датаПриемки-caption")
      data-test-i-i-s-k-r-osipova-itog-акт-приемки-e-датаПриемки=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "датаПриемки" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.сотрудники.должность
      class=(if (v-get validationModel.сотрудники "должность" "isInvalid") "error")
      label=(t "forms.i-i-s-k-r-osipova-itog-акт-приемки-e.сотрудники.должность-caption")
      data-test-i-i-s-k-r-osipova-itog-акт-приемки-e-сотрудники-должность=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.сотрудники "должность" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.сотрудники.фИО
      class=(if (v-get validationModel.сотрудники "фИО" "isInvalid") "error")
      label=(t "forms.i-i-s-k-r-osipova-itog-акт-приемки-e.сотрудники.фИО-caption")
      data-test-i-i-s-k-r-osipova-itog-акт-приемки-e-сотрудники-фИО=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.сотрудники "фИО" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
